<%= form_tag sessions_path do %>
<p>
	<%= label_tag :username %>
	<%= text_field_tag :username, params[:username] %>
</p>
<p>
	<%= label_tag :password %>
	<%= password_field_tag :password %>
</p>
<%= submit_tag "Log in", class: "btn btn-info" %>
<% end %>
<p>
	New user? <%= link_to "Sign up now!", sign_up_path %>
</p>
